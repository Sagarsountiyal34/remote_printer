<div class="modal" id="phoneNumberVerification" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="phoneNumberVerification" style="max-width:1000px;">
        <div class="modal-content" id="phoneNumberVerificationModalContent">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Upload Document into group</h5>
                <button type="button" class="close" data-dismiss="modal" id="close_history_modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="card mb-3">
                    <div class="card-header">
                        <i class="fas fa-table"></i>
                        Phone Number Verification</div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <span id="phone_number_span">Enter Phone number: <input type="text" name="phone_number" id="phone_number" pattern="[9]{1}[1]{1}[7-9]{1}[0-9]{9}" title="Phone number with 91(country code) and 7-9 and remaing 9 digit with 0-9"><br><br></span>
                            <span style="display:none;" id="otp_span">Enter OTP: <input type="number" name="otp" id="otp"><br><br></span>
                            <%= button_tag "Get OTP", :class => 'btn_1', :id => 'send_otp' %>
                            <%= button_tag "Create Account and save Doc", :class => 'btn_1', :id => 'create_account_with_doc', :style => 'display:none;' %>
                        </div>
                    </div>
                    <div class="card-footer small text-muted">
                    </div>
                    <div id="loader_wrap_from_history" class="loader_wrap">
                        <img src="/images/loader.gif" id="loader_img" width="100" height="100">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



